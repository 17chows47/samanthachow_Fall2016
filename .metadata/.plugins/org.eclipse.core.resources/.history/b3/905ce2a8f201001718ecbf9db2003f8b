package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
	int row;
	int column;
	
    @Override
    public int getRow(){
        // TODO Auto-generated method stub
        return row;
    }

    @Override
    public int getCol(){
        // TODO Auto-generated method stub
        return column;
    } 
    public static String[] parseCell(String cellName){
        String cell[] = new String[2];
 		cell[0] = cellName.charAt(0) + "";
        cell[1] = cellName.charAt(1) + cellName.charAt(1) + "";
        return cell;
    }

    public SpreadsheetLocation(String cellName){
    	parseCell(cellName);
    }

}
